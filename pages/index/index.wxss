/* pages/index/index.wxss */
.swiper_wrap{
  width: 750rpx;
  height: 340rpx;
}
.swiper_wrap navigator img{
  width: 100%;
}
.nav_wrap{
  display: flex;
}
.nav_wrap .nav_item{
  flex: 1;
  padding: 20rpx;
}
.nav_wrap .nav_item img{
  width: 100%;
}
.floor_wrap{
  padding-top: 20rpx;
  background-color: #eee;
}
.floor_wrap .floor_item .floor_title{
  padding: 10rpx 0;
}
.floor_wrap .floor_item .floor_title image{
  width: 100%;
}
.floor_wrap .floor_item .floor_content{
  overflow: hidden;
  background-color: #fff;
}
.floor_wrap .floor_item .floor_content .content_item{
  float: left;
  width: 33.33%;
}
.floor_wrap .floor_item .floor_content .content_item:nth-last-child(-n+4){
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_wrap .floor_item .floor_content .content_item:nth-child(2),
.floor_wrap .floor_item .floor_content .content_item:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_wrap .floor_item .floor_content .content_item image{
  width: 100%;
  height: 100%;
}



